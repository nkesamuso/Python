class Dog:
   
    def __init__(self, name, age):
        self.name = name  
        self.age = age    
    

    def bark(self):
        print(f"{self.name} says: Woof!")
    
    def get_info(self):
        return f"{self.name} is {self.age} years old"

# Creating objects (instances)
dog1 = Dog("Buddy", 3)
dog2 = Dog("Max", 5)

print(dog1.name)  
print(dog2.age)   # 

dog1.bark()  
print(dog2.get_info()) 
